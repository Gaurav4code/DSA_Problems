class Solution {
public:
    int minPartitions(string n) {
        int m=INT_MIN;
        
        
        
        for(int i=0;i<n.size();i++){
            
            int t=n[i]-'0';
            
            
            if(t>m){
                m=t;
            }
        }
        
        return m;
    }
};
