class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        
        
        
        vector<int> v;
        map<int,int> m;
        
        for(int i=0;i<nums.size();i++){
            if(nums[i])
             v.push_back(nums[i]);
        }
        for(int i=0;i<nums.size();i++){
            if(!nums[i])
             v.push_back(nums[i]);
        }
        nums=v;
    }
};
