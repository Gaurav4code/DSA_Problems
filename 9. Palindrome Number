class Solution {
public:
    bool isPalindrome(int x) 
    {
    int d=abs(x);
  int a=1;
        string s =to_string(x);
        for(int i=0;i<s.length();i++){
            if(s[i]!=s[s.length()-i-1])
                a= 0;
        }
   if(x<0||a==0){
        return false;
      }
      return true;
    }
};
